{
    "name": "flash_coffe-berhasil",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "debug": "npm run build-ts && npm run watch-debug",
        "build-ts": "npm run build-ts-tsc && npm run tslint",
        "tslint": "tslint -c tslint.json -p tsconfig.json",
        "build-ts-tsc": "tsc",
        "prettier": "prettier --config ./.prettierrc --write \"**/*.js\"",
        "pretest": "eslint --ignore-path .gitignore .",
        "lint": "yarn prettier --write --check --config ./.prettierrc \"**/*.js\" && eslint --fix ./src",
        "release": "release-it patch --no-npm.publish",
        "serve-debug": " nodemon --inspect --optimize_for_size --max_old_space_size=920 dist/index.js",
        "watch-ts": "tsc -w",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\""
    },
    "author": "bramastavikana",
    "license": "ISC",
    "dependencies": {
        "@babel/cli": "^7.16.0",
        "@babel/core": "^7.16.0",
        "@babel/node": "^7.16.0",
        "@babel/preset-env": "^7.16.4",
        "aws-sdk": "^2.1194.0",
        "babel-preset-env": "^1.7.0",
        "concurrently": "^7.3.0",
        "cors": "^2.8.5",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-router": "0.0.1",
        "fs": "^0.0.1-security",
        "multer": "^1.4.5-lts.1",
        "mysql": "^2.18.1",
        "mysql2": "^2.3.3",
        "nodemon": "^2.0.15",
        "tslint": "^6.1.3",
        "typescript": "^4.4.4"
    },
    "devDependencies": {
        "@types/mysql": "^2.15.21"
    }
}
